#include ".Xcolors"

! █▓▒░ fonts
urxvt*font: xft:gohufont:size=11:antialias=false, \
            xft:Symbola:pixelsize=14:antialias=true:hinting=slight, \
            xft:Terminess Powerline:size=11

urxvt*boldFont:
urxvt*italicFont:
urxvt*bolditalicFont:

urxvt*letterSpace: 0
urxvt*lineSpace: -1

! █▓▒░ interface
urxvt*scrollBar: false
urxvt*transparent: false
!urxvt*shading: 38

! █▓▒░ misc
urxvt.saveLines: 9999
urxvt.iso14755: false

! █▓▒░ clipboard
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
! some other extensions: tabbed
URxvt.perl-ext-common: default,clipboard,matcher,keyboard-select,url-select,digital-clock

URxvt.matcher.button: 1

! █▓▒░ cursor
URxvt.cursorBlink: true

! Pointer
URxvt.pointerBlank: true
